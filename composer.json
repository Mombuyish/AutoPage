{
    "name": "yish/autopage",
    "description": "Auto load static page and generate routes for laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yish",
            "email": "mombuartworks@gmail.com"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "5.*|^6.0",
        "illuminate/view": "5.*|^6.0",
        "illuminate/config": "5.*|^6.0",
        "illuminate/routing": "5.*|^6.0",
        "illuminate/http": "5.*|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Yish\\AutoPage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yish\\AutoPage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "component": "package",
        "laravel": {
            "providers": [
                "Yish\\AutoPage\\AutoPageServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
